{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
    ":semanticCommits",
    ":automergeDigest",
    ":automergeMinor"
  ],
  "timezone": "UTC",
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "automerge": true,
  "automergeType": "pr",
  "automergeStrategy": "auto",
  "platformAutomerge": true,
  "rebaseWhen": "behind-base-branch",
  "packageRules": [
  ]
}
